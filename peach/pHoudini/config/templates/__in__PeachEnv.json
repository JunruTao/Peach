{
    "author" : "${AUTHOR}",
    "version": "${PACKAGE_VERSION}",
    "load_package_once": true,
    "enable" : "houdini_version >= '${HOU_MIN_VER}' and houdini_version < '${HOU_MAX_VER}'",
    "env" : [
        {"PEACH_VERSION" : "${PEACH_VERSION}"},
        {"PEACH_VERSION_PKG": "${PACKAGE_VERSION}"},
        {"PEACH_CREATOR" : "${AUTHOR}"},
        {"PEACH_BUILD" : "${PEACH_BUILD}"},
        {"PEACH_PACKAGES_PATH" : "$PEACH_HOU/packages"},
        {"PEACH_ICONS" : "$PEACH/icons/SVG"},
        {"PEACH_HICON" : "$PEACH/icons/HICON"},
        {"PEACH_IMAGE" : "$PEACH/images"},
        {"PEACH_HIMGS" : "$PEACH/images/HIMG"}
    ],
    "path" : "$PEACH_HOU/PeachEnv"
}

